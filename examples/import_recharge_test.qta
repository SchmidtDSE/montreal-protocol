start default

  define application "ImportRechargeTest"
  
    uses substance "TestSubstance"
      initial charge with 5 kg / unit for import
      enable import
      recharge 10% each year with 0.5 kg / unit
      retire 0% each year
      set import to 100 kg during year 1
      equals 2 tCO2e / kg
    end substance
  
  end application

end default

start simulations

  simulate "BAU"
  from years 1 to 5

end simulations
